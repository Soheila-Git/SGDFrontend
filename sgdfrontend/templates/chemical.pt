<div metal:use-macro="layout">
  <div metal:fill-slot="content">


<body>

<!-- Subhead
================================================== -->
<header class="jumbotron subhead" id="overview">
	<div class="container">
		<h1>Chemical: <span tal:content="chemical['display_name']"></h1>
	</div>
</header>


<div class="container-fluid">

<!-- Docs nav
================================================== -->
<div class="row-fluid">
	<div class="span3 bs-docs-sidebar">
		<ul class="nav nav-list bs-docs-sidenav affix-top" id='navbar' name='navbar'>
			<li><h3><span tal:content="chemical['display_name']"></h3></li>
			<li><a href="#abstract"><i class="icon-chevron-right"></i> Abstract</a></li>
			<li><a href="#cited_for_annotations"><i class="icon-chevron-right"></i> Cited For Annotations</a></li>
		</ul>
	</div>
	
	<div class="span9">  

<!-- Abstract
================================================== -->
<section id="abstract">
	<div class="page-header">
		<h1>Summary</h1>
		<div>
			<ul class='unstyled'>
				<li tal:condition="python: chemical['aliases'] != ''"><strong>Aliases: </strong><span tal:content="chemical['aliases']"></li>
			</ul>
		</div>
	</div>
</section>

<!-- Phenotypes
================================================== -->
<section id="phenotypes">
	<div class="page-header">
		<h1>Phenotypes <span id='phenotype_header'></span></h1>
		<div id='phenotype_wrapper'>
		<p id='phenotype_message' style="display:none;">No phenotype information curated from this chemical.</p>
			<table class='table table-striped table-bordered table-condensed tablesorter' id='phenotype_table' width="100%">
			<thead>
  			<tr>
    			<th>Phenotype</th>
    			<th>Gene</th>
    			<th>Qualifier</th>
    			<th>Mutant Type</th>
  			</tr>
  			</thead>
			</table>
			<br></br>
		</div>
	</div>
</section>


<script>
	
	function set_up_phenotype_table() {
		var options = {};
		options["bPaginate"] = true;
		options["sPaginationType"] = "bootstrap";
		options["bProcessing"] = true
        options["oTableTools"] = table_tool_option('${phenotype_filename}', false);
		options["sDom"] = '<"clear">lfrtipT';
		options["aaSorting"] = [[0, "asc"]];
        options["fnInitComplete"] = set_up_message_and_count("phenotype_header", "phenotype_message", "phenotype_wrapper");
		options["fnDrawCallback"] = set_up_details(get_details);
		options["aoColumns"] = [null, null, null, null]
		options["sAjaxSource"] = '${phenotype_overview_table_link}';
	}
	
	$(document).ready(function() {
		set_up_phenotype_table();
	});
</script>

</div>

</body>
</div>
</div>

