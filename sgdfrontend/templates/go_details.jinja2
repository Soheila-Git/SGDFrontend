<!DOCTYPE html>

<title>{{display_name}} Gene Ontology</title>

{% extends "templates/global_layout.jinja2" %}


{% block body %}

<div id="center_title" class="row">
	<h2><span class="gene_name">{{display_name}}/{{format_name}}</span> Gene Ontology Annotations  <a href="http://www.yeastgenome.org/help/GO.html" name="top" class="help"><img src="http://www.yeastgenome.org/images/icon_help_circle_dark.png" alt="Help"></a></h2>
</div>


<!-- Includes tabs.html into template -->
{% include "templates/tabs.jinja2" %}
<script>document.getElementById('go_tab').id = 'current';</script>

<div class="row">
	
<!-- Includes navbar.html into template -->
{% include "templates/navbar.jinja2" %}

<div class="small-12 large-9 columns content-column"> 
	
<!-- Includes go_overview.html into template -->
{% include "templates/go_overview.jinja2" %}

<!-- GO
================================================== -->
<section id="gos" data-magellan-destination="gos">
	<div class="page-header">
		<h1>Gene Ontology Terms <span class="round secondary label"><span id="evidence_header"></span> <small>entries for</small> <span id="go_header_id"></span> <small>gene ontology terms</small></span></h1>
		<p id='evidence_message' style='display:none'>No gene ontology terms curated.</p>
		<div id='evidence_wrapper'>
			<div class="button-bar">
				<ul class="button-group radius">
					<li><a disabled="true" id="evidence_table_download" class="small button secondary"><i class='icon-download-alt'></i> Download</a></li>
				</ul>
			</div>
			
			<table class='table table-striped table-bordered table-condensed' id='evidence_table' width="100%">
			<thead>
  			<tr>
  				<th>Gene</th>
  				<th>Gene Format Name</th>
  				<th>Gene Ontology Term</th>
  				<th>Aspect</th>
   				<th>Method</th>
  				<th>Evidence</th>
  				<th>Relationships</th>
  				<th>Source</th>
    			<th>Reference</th>
  			</tr>
  			</thead>
			</table>
        </div>
   </div>
   <br>
	<br>
	<br>
	<br>
	<br>
	<br>
</section>

</div>
</div>
{% endblock body %}

{% block scripts %}
<script src="/static/js/local.js"></script>
<script src="/static/js/go_details.js"></script>

<script>
	//Template links and filenames
	var go_overview_link = "{{go_overview_link}}";
	var go_details_link = "{{go_details_link}}";
	var download_table_link = "{{download_table_link}}";
	var download_table_filename = "{{go_details_filename}}";
		
	$(document).ready(function() {
		//Get evidence data
  		$.getJSON(go_details_link, function(data) {
  			var header_id = "evidence_header";
  			var phenotype_header_id = "go_header_id";
  			var table_id = "evidence_table";
  			var download_button_id = "evidence_table_download";
  			
  			var message_id = "evidence_message";
  			var wrapper_id = "evidence_wrapper";
  			
  			if(data.length > 0) {
  				document.getElementById()
  				set_up_evidence_table(header_id, phenotype_header_id, table_id, download_button_id, 
  				download_table_link, download_table_filename, data);
  			}
  			else {
  				document.getElementById(message_id).style.display = "block";
  				document.getElementById(wrapper_id).style.display = "none";
  				document.getElementById(header_id).innerHTML = 0;
  				document.getElementById(phenotype_header_id).innerHTML = 0;
  			}
  			
  		});
	});
</script>
        

{% endblock scripts %}
