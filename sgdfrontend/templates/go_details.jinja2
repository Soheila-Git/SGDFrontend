{% block scripts %}
<script src="/static/js/local.js"></script>
<script src="/static/js/go_details.js"></script>

<script>
	//Template links and filenames
	var go_overview_link = "{{go_overview_link}}";
	var go_details_link = "{{go_details_link}}";
	var download_table_link = "{{download_table_link}}";

	var bp_download_table_filename = "{{ locus.display_name }}_manual_bp_go";
	var mf_download_table_filename = "{{ locus.display_name }}_manual_mf_go";
	var cc_download_table_filename = "{{ locus.display_name }}_manual_cc_go";
    var htp_bp_download_table_filename = "{{ locus.display_name }}_htp_bp_go";
	var htp_mf_download_table_filename = "{{ locus.display_name }}_htp_mf_go";
	var htp_cc_download_table_filename = "{{ locus.display_name }}_htp_cc_go";
    var comp_bp_download_table_filename = "{{ locus.display_name }}_computational_bp_go";
	var comp_mf_download_table_filename = "{{ locus.display_name }}_computational_mf_go";
	var comp_cc_download_table_filename = "{{ locus.display_name }}_computational_cc_go";

	$(document).ready(function() {
		//Get evidence data
  		$.getJSON(go_details_link, function(data) {
  			var bp_header_id = "bp_evidence_header";
  			var bp_go_header_id = "bp_go_header_id";
  			var bp_table_id = "bp_evidence_table";
  			var bp_download_button_id = "bp_evidence_table_download";
  			var bp_message_id = "bp_evidence_message";
  			var bp_wrapper_id = "bp_evidence_wrapper";

            var htp_bp_header_id = "htp_bp_evidence_header";
  			var htp_bp_go_header_id = "htp_bp_go_header_id";
  			var htp_bp_table_id = "htp_bp_evidence_table";
  			var htp_bp_download_button_id = "htp_bp_evidence_table_download";
  			var htp_bp_message_id = "htp_bp_evidence_message";
  			var htp_bp_wrapper_id = "htp_bp_evidence_wrapper";

            var comp_bp_header_id = "comp_bp_evidence_header";
  			var comp_bp_go_header_id = "comp_bp_go_header_id";
  			var comp_bp_table_id = "comp_bp_evidence_table";
  			var comp_bp_download_button_id = "comp_bp_evidence_table_download";
  			var comp_bp_message_id = "comp_bp_evidence_message";
  			var comp_bp_wrapper_id = "comp_bp_evidence_wrapper";

  			var bpData = data['biological_process'];
  			set_up_evidence_table(bp_header_id, bp_go_header_id, bp_table_id, bp_message_id, bp_wrapper_id, bp_download_button_id, bp_download_table_filename, 'manually curated',
                    download_table_link, bpData);

            set_up_evidence_table(htp_bp_header_id, htp_bp_go_header_id, htp_bp_table_id, htp_bp_message_id, htp_bp_wrapper_id, htp_bp_download_button_id, htp_bp_download_table_filename, 'high-throughput',
                    download_table_link, bpData);

            set_up_evidence_table(comp_bp_header_id, comp_bp_go_header_id, comp_bp_table_id, comp_bp_message_id, comp_bp_wrapper_id, comp_bp_download_button_id, comp_bp_download_table_filename, 'computational',
  				    download_table_link, bpData);

  			var mf_header_id = "mf_evidence_header";
  			var mf_go_header_id = "mf_go_header_id";
  			var mf_table_id = "mf_evidence_table";
  			var mf_download_button_id = "mf_evidence_table_download";
  			var mf_message_id = "mf_evidence_message";
  			var mf_wrapper_id = "mf_evidence_wrapper";

            var htp_mf_header_id = "htp_mf_evidence_header";
  			var htp_mf_go_header_id = "htp_mf_go_header_id";
  			var htp_mf_table_id = "htp_mf_evidence_table";
  			var htp_mf_download_button_id = "htp_mf_evidence_table_download";
  			var htp_mf_message_id = "htp_mf_evidence_message";
  			var htp_mf_wrapper_id = "htp_mf_evidence_wrapper";

            var comp_mf_header_id = "comp_mf_evidence_header";
  			var comp_mf_go_header_id = "comp_mf_go_header_id";
  			var comp_mf_table_id = "comp_mf_evidence_table";
  			var comp_mf_download_button_id = "comp_mf_evidence_table_download";
  			var comp_mf_message_id = "comp_mf_evidence_message";
  			var comp_mf_wrapper_id = "comp_mf_evidence_wrapper";
  			
  			var mfData = data['molecular_function'];
  			set_up_evidence_table(mf_header_id, mf_go_header_id, mf_table_id, mf_message_id, mf_wrapper_id, mf_download_button_id, mf_download_table_filename, 'manually curated',
  				    download_table_link, mfData);

            set_up_evidence_table(htp_mf_header_id, htp_mf_go_header_id, htp_mf_table_id, htp_mf_message_id, htp_mf_wrapper_id, htp_mf_download_button_id, htp_mf_download_table_filename, 'high-throughput',
  				    download_table_link, mfData);

            set_up_evidence_table(comp_mf_header_id, comp_mf_go_header_id, comp_mf_table_id, comp_mf_message_id, comp_mf_wrapper_id, comp_mf_download_button_id, comp_mf_download_table_filename, 'computational',
  				    download_table_link, mfData);
  			
  			var cc_header_id = "cc_evidence_header";
  			var cc_go_header_id = "cc_go_header_id";
  			var cc_table_id = "cc_evidence_table";
  			var cc_download_button_id = "cc_evidence_table_download";
  			var cc_message_id = "cc_evidence_message";
  			var cc_wrapper_id = "cc_evidence_wrapper";

            var htp_cc_header_id = "htp_cc_evidence_header";
  			var htp_cc_go_header_id = "htp_cc_go_header_id";
  			var htp_cc_table_id = "htp_cc_evidence_table";
  			var htp_cc_download_button_id = "htp_cc_evidence_table_download";
  			var htp_cc_message_id = "htp_cc_evidence_message";
  			var htp_cc_wrapper_id = "htp_cc_evidence_wrapper";

            var comp_cc_header_id = "comp_cc_evidence_header";
  			var comp_cc_go_header_id = "comp_cc_go_header_id";
  			var comp_cc_table_id = "comp_cc_evidence_table";
  			var comp_cc_download_button_id = "comp_cc_evidence_table_download";
  			var comp_cc_message_id = "comp_cc_evidence_message";
  			var comp_cc_wrapper_id = "comp_cc_evidence_wrapper";
  			
  			var ccData = data['cellular_component'];
  			set_up_evidence_table(cc_header_id, cc_go_header_id, cc_table_id, cc_message_id, cc_wrapper_id, cc_download_button_id, cc_download_table_filename, 'manually curated',
  				    download_table_link, ccData);

            set_up_evidence_table(htp_cc_header_id, htp_cc_go_header_id, htp_cc_table_id, htp_cc_message_id, htp_cc_wrapper_id, htp_cc_download_button_id, htp_cc_download_table_filename, 'high-throughput',
  				    download_table_link, ccData);

            set_up_evidence_table(comp_cc_header_id, comp_cc_go_header_id, comp_cc_table_id, comp_cc_message_id, comp_cc_wrapper_id, comp_cc_download_button_id, comp_cc_download_table_filename, 'computational',
  				    download_table_link, ccData);
  			
  		});
	});
</script>
        

{% endblock scripts %}
