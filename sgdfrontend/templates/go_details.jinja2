	<br>
	<br>
	<br>
</section>

</div>
</div>
{% endblock body %}

{% block scripts %}
<script src="/static/js/local.js"></script>
<script src="/static/js/go_details.js"></script>

<script>
	//Template links and filenames
	var go_overview_link = "{{go_overview_link}}";
	var go_details_link = "{{go_details_link}}";
	var download_table_link = "{{download_table_link}}";
	var bp_download_table_filename = "{{bp_go_details_filename}}";
	var mf_download_table_filename = "{{mf_go_details_filename}}";
	var cc_download_table_filename = "{{cc_go_details_filename}}";
		
	$(document).ready(function() {
		//Get evidence data
  		$.getJSON(go_details_link, function(data) {
  			var bp_header_id = "bp_evidence_header";
  			var bp_go_header_id = "bp_go_header_id";
  			var bp_table_id = "bp_evidence_table";
  			var bp_download_button_id = "bp_evidence_table_download";
  			
  			var bp_message_id = "bp_evidence_message";
  			var bp_wrapper_id = "bp_evidence_wrapper";
  			
  			var bpData = data['biological_process'];
  			if(bpData.length > 0) {
  				document.getElementById()
  				set_up_evidence_table(bp_header_id, bp_go_header_id, bp_table_id, bp_download_button_id, 
  				download_table_link, bp_download_table_filename, bpData);
  			}
  			else {
  				document.getElementById(bp_message_id).style.display = "block";
  				document.getElementById(bp_wrapper_id).style.display = "none";
  				document.getElementById(bp_header_id).innerHTML = 0;
  				document.getElementById(bp_go_header_id).innerHTML = 0;
  			}
  			
  			var mf_header_id = "mf_evidence_header";
  			var mf_go_header_id = "mf_go_header_id";
  			var mf_table_id = "mf_evidence_table";
  			var mf_download_button_id = "mf_evidence_table_download";
  			
  			var mf_message_id = "mf_evidence_message";
  			var mf_wrapper_id = "mf_evidence_wrapper";
  			
  			var mfData = data['molecular_function'];
  			if(mfData.length > 0) {
  				document.getElementById()
  				set_up_evidence_table(mf_header_id, mf_go_header_id, mf_table_id, mf_download_button_id, 
  				download_table_link, mf_download_table_filename, mfData);
  			}
  			else {
  				document.getElementById(mf_message_id).style.display = "block";
  				document.getElementById(mf_wrapper_id).style.display = "none";
  				document.getElementById(mf_header_id).innerHTML = 0;
  				document.getElementById(mf_go_header_id).innerHTML = 0;
  			}
  			
  			var cc_header_id = "cc_evidence_header";
  			var cc_go_header_id = "cc_go_header_id";
  			var cc_table_id = "cc_evidence_table";
  			var cc_download_button_id = "cc_evidence_table_download";
  			
  			var cc_message_id = "cc_evidence_message";
  			var cc_wrapper_id = "cc_evidence_wrapper";
  			
  			var ccData = data['cellular_component'];
  			if(ccData.length > 0) {
  				document.getElementById()
  				set_up_evidence_table(cc_header_id, cc_go_header_id, cc_table_id, cc_download_button_id, 
  				download_table_link, cc_download_table_filename, ccData);
  			}
  			else {
  				document.getElementById(cc_message_id).style.display = "block";
  				document.getElementById(cc_wrapper_id).style.display = "none";
  				document.getElementById(cc_header_id).innerHTML = 0;
  				document.getElementById(cc_go_header_id).innerHTML = 0;
  			}
  			
  		});
	});
</script>
        

{% endblock scripts %}
