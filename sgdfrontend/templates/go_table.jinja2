<!-- GO
================================================== -->
<section id="{{table_type}}_go" data-magellan-destination="{{table_type}}_go">
	<div class="page-header">
		<{% if small %}h2{% else %}h1{% endif %}>{{table_name}}
            <span class="round secondary label">
                <span id="{{table_type}}_go_header"></span>
                <small>entries for</small>
                <span id="{{table_type}}_go_subheader"></span>
                <small id="{{table_type}}_go_subheader_type"></small>
            </span>
		</{% if small %}h2{% else %}h1{% endif %}>

		<div class="button-bar">
			<ul class="button-group radius">
				<li><a disabled="true" id="{{table_type}}_go_table_download" class="small button secondary"><i class="icon-download-alt"></i> Download</a></li>
			</ul>

			<ul class="button-group radius">
				<li><a disabled="true" id="{{table_type}}_go_table_analyze" class="small button secondary"><i class="icon-briefcase"></i> Analyze</a></li>
			</ul>

            <ul {% if child_count == 0 or count == child_count %} style="display:none" {% endif %} class="button-group radius">
			    <li><a disabled="true" id="more_genes_button" class="small button secondary">Add Genes Associated With Child Terms</a></li>
		    </ul>
		</div>

		<table class="table table-striped table-bordered table-condensed" id="{{table_type}}_go_table" width="100%">
			<thead>
                <tr>
                    <th>Evidence ID</th>
                    <th>Analyze ID</th>
                    <th></th>
                    <th>Gene</th>
                    <th>Gene Format Name</th>
                    <th>Gene Ontology Term</th>
                    <th>Qualifier</th>
                    <th>Method</th>
                    <th>Evidence</th>
                    <th>Source</th>
                    <th>Assigned On</th>
                    <th>Reference</th>
                    <th>Relationships</th>
                </tr>
  			</thead>
		</table>
    </div>
</section>