<div metal:use-macro="layout">
  <div metal:fill-slot="content">


<body>
	
<!-- Subhead
================================================== -->
<header class="jumbotron subhead" id="overview">
	<div class="container">
		<h1><span tal:content="structure display_name"></h1>
	</div>
</header>

<div class="container-fluid">


<!-- Docs nav
================================================== -->
<div class="row-fluid">
	<div class="span3 bs-docs-sidebar">
		<ul class="nav nav-list bs-docs-sidenav" id='navbar' name='navbar' style="margin-left:auto; margin-right:auto">
			<li><h4><span tal:content="structure name_with_link"></h4></li>
			<li><a href="#evidence"><i class="icon-chevron-right"></i> Evidence</a></li>
          	<li><a href="#literature"><i class="icon-chevron-right"></i> References</a></li>
		</ul>
	</div>
	
	<div class="span9">  


<!-- Evidence
================================================== -->
<section id="evidence">
	<div class="page-header">
		<h1>Evidence <span id='evidence_header'></span></h1>
		<span>Source: SGD</span>
		
		<div id='undivided' tal:condition='python: not split'>
		<span id='evidence_message' class="hide">No evidence curated.</span>
		<div id='evidence_wrapper'>		
			<table class='table table-striped table-bordered table-condensed tablesorter' id='evidence_table' width="100%">
			<thead>
  			<tr>
  				<th></th>
  				<th>Phenotype</th>
  				<th>Gene</th>
    			<th>Qualifier</th>
    			<th>Mutant</th>
    			<th>Experiment</th>
    			<th>Allele</th>
    			<th>Reporter</th>
    			<th>Chemical</th>
    			<th>Experiment Details</th>
    			<th>Condition</th>
    			<th>Details</th>
    			<th>Strain</th>
    			<th>Source</th>
    			<th>Reference</th>
  			</tr>
  			</thead>
  			</table>
        </div>
       </div>
		<div id='divided' tal:condition='python: split'>
		<h3>Cellular Phenotype Evidence <span id='cellular_evidence_header'></span></h3>
		<span id='cellular_evidence_message' class="hide">No cellular phenotypes curated.</span>
		<div id='evidence_wrapper'>		
			<table class='table table-striped table-bordered table-condensed tablesorter' id='cellular_evidence_table' width="100%">
			<thead>
  			<tr>
  				<th></th>
  				<th>Phenotype</th>
  				<th>Gene</th>
    			<th>Qualifier</th>
    			<th>Mutant</th>
    			<th>Experiment</th>
    			<th>Allele</th>
    			<th>Reporter</th>
    			<th>Chemical</th>
    			<th>Experiment Details</th>
    			<th>Condition</th>
    			<th>Details</th>
    			<th>Strain</th>
    			<th>Source</th>
    			<th>Reference</th>
  			</tr>
  			</thead>
  			</table>
        </div>
		<br></br>
		
		<h3>Chemical Phenotype Evidence <span id='chemical_evidence_header'></span></h3>
		<span id='chemical_evidence_message' class="hide">No chemical phenotypes curated.</span>
		<div id='chemical_evidence_wrapper'>		
			<table class='table table-striped table-bordered table-condensed tablesorter' id='chemical_evidence_table' width="100%">
			<thead>
  			<tr>
  				<th></th>
  				<th>Phenotype</th>
  				<th>Gene</th>
    			<th>Qualifier</th>
    			<th>Mutant</th>
    			<th>Experiment</th>
    			<th>Allele</th>
    			<th>Reporter</th>
    			<th>Chemical</th>
    			<th>Experiment Details</th>
    			<th>Condition</th>
    			<th>Details</th>  	
    			<th>Strain</th>
    			<th>Source</th>
    			<th>Reference</th>		
    		</tr>
  			</thead>
  			</table>
        </div>
		<br></br>
		
		<h3>Protein/Peptide and RNA Phenotype Evidence <span id='pp_rna_evidence_header'></span></h3>
		<span id='pp_rna_evidence_message' class="hide">No protein/peptide and RNA phenotypes curated.</span>
		<div id='pp_rna_evidence_wrapper'>		
			<table class='table table-striped table-bordered table-condensed tablesorter' id='pp_rna_evidence_table' width="100%">
			<thead>
  			<tr>
  				<th></th>
  				<th>Phenotype</th>
  				<th>Gene</th>
    			<th>Qualifier</th>
    			<th>Mutant</th>
    			<th>Experiment Type</th>
    			<th>Allele</th>
    			<th>Reporter</th>
    			<th>Chemical</th>
    			<th>Experiment Details</th>
    			<th>Condition</th>
    			<th>Details</th>
    			<th>Strain</th>
    			<th>Source</th>
    			<th>Reference</th>
  			</tr>
  			</thead>
  			</table>
        </div>
		<br></br>
	</div>
	
	<div id="cytoscape_vis">
		<h3>Cytoscape Visualization</h3>
		<div id="cytoscapeweb">
            Cytoscape visualization is loading...
        </div>
        <span>Export:</span>
        <div class="btn-group">
        	<a class="btn" onclick="vis.exportNetwork('png', '/download_graph/png')">png</a>
        	<a class="btn" onclick="vis.exportNetwork('svg', '/download_graph/svg')">svg</a>
        	<a class="btn" onclick="vis.exportNetwork('pdf', '/download_graph/pdf')">pdf</a>
        	<a class="btn" onclick="vis.exportNetwork('xgmml', '/download_graph/xml')">xgmml</a>
        	<a class="btn" onclick="vis.exportNetwork('graphml', '/download_graph/txt')">graphml</a>
        	<a class="btn" onclick="vis.exportNetwork('sif', '/download_graph/txt', options={nodeAttr:'label', interactionAttr:'label'})">txt</a>

        </div>
        </div>
		
	</div>
</section>

<!-- References
================================================== -->
<section id="literature">
	<div class="page-header">
		<h1>References</h1>
		<ul id='reference_list'>
		</ul>
	</div>
</section>

<script>
	function get_cell_details(row) {
		var description = '';
		if(row[7] != null) {
            description = description + '<strong>Reporter:</strong> ' + row[7] + '<br>';
        }
        if(row[8] != null) {
            description = description + '<strong>Chemical:</strong> ' + row[8] + '<br>';
        }
        if(row[9] != null) {
        	description = description + '<strong>Experiment Details:</strong> ' + row[9] + '<br>';
        }
        if(row[10] != null) {
            description = description + '<strong>Condition:</strong> ' + row[10] + '<br>';
        }
        if(row[11] != null) {
            description = description + '<strong>Details:</strong> ' + row[11] + '<br>';
        }
	
  		if(description == '') {
  			description = null;
  	    }
  	    return description;
  	}
  	
  	function get_chem_details(row) {
  		var description = '';
  	
  		if(row[7] != null) {
            description = description + '<strong>Reporter:</strong> ' + row[7] + '<br>';
        }
        if(row[9] != null) {
            description = description + '<strong>Experiment Details:</strong> ' + row[9] + '<br>';
        }
        if(row[10] != null) {
            description = description + '<strong>Condition:</strong> ' + row[10] + '<br>';
        }
        if(row[11] != null) {
            description = description + '<strong>Details:</strong> ' + row[11] + '<br>';
        }
  	
  		if(description == '') {
  			description = null;
  	    }
  	    return description;
  	}
  	
  	function get_pp_details(row) {
  		var description = '';
  		if(row[8] != null) {
			description = description + '<strong>Chemical:</strong> ' + row[8] + '<br>';
		}
		if(row[9] != null) {
			description = description + '<strong>Experiment Details:</strong> ' + row[9] + '<br>';
		}
		if(row[10] != null) {
			description = description + '<strong>Condition:</strong> ' + row[10] + '<br>';
		}
		if(row[11] != null) {
			description = description + '<strong>Details:</strong> ' + row[11] + '<br>';
		}
		
		if(description == '') {
  			description = null;
  	    }
  	    return description;
  	}
	
	function set_up_cell_table(data) {
		//Insert a 'details' column to the table  		
  		add_detail_column('cellular_evidence_table');
	
		//Set up cellular evidence table
		var options = {};
		options["bPaginate"] = false;
        options["oTableTools"] = table_tool_option('${cellular_phenotype_evidence_filename}', true, 0, 2);
		options["sDom"] = '<"clear">lfrtipT';
		options["aaSorting"] = [[0, "asc"]];
        options["fnInitComplete"] = set_up_message_and_count("cellular_evidence_header", "cellular_evidence_message", "cellular_evidence_wrapper");
		options["fnDrawCallback"] = set_up_details(get_cell_details);
		options["aoColumns"] = [{"bSortable":false}, null, {"bSearchable":false, "bVisible":false}, null, null, null, null, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, null, {"bSearchable":false, "bVisible":false}, null]
		options["aaData"] = data;
		$('#cellular_evidence_table').dataTable(options);
	}
	
	function set_up_chem_table(data) {
		//Insert a 'details' column to the table  		
  		add_detail_column('chemical_evidence_table');
	
		//Set up cellular evidence table
		var options = {};
		options["bPaginate"] = false;
        options["oTableTools"] = table_tool_option('${chemical_phenotype_evidence_filename}', true, 1, 2);
		options["sDom"] = '<"clear">lfrtipT';
		options["aaSorting"] = [[7, "asc"]];
        options["fnInitComplete"] = set_up_message_and_count("chemical_evidence_header", "chemical_evidence_message", "chemical_evidence_wrapper");
		options["fnDrawCallback"] = set_up_details(get_chem_details);
		options["aoColumns"] = [{"bSortable":false}, null, {"bSearchable":false, "bVisible":false}, null, null, null, null, {"bSearchable":false, "bVisible":false}, null, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, null, {"bSearchable":false, "bVisible":false}, null]
		options["aaData"] = data;
		$('#chemical_evidence_table').dataTable(options);
	} 
	
	function set_up_pp_table(data) {
		//Insert a 'details' column to the table  		
  		add_detail_column('pp_rna_evidence_table');
	
		//Set up cellular evidence table
		var options = {};
		options["bPaginate"] = false;
        options["oTableTools"] = table_tool_option('${pp_rna_phenotype_evidence_filename}', true, 2, 2);
		options["sDom"] = '<"clear">lfrtipT';
		options["aaSorting"] = [[6, "asc"]];
        options["fnInitComplete"] = set_up_message_and_count("pp_rna_evidence_header", "pp_rna_evidence_message", "pp_rna_evidence_wrapper");
		options["fnDrawCallback"] = set_up_details(get_pp_details);
		options["aoColumns"] = [{"bSortable":false}, null, {"bSearchable":false, "bVisible":false}, null, null, null, null, null, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, null, {"bSearchable":false, "bVisible":false}, null]
		options["aaData"] = data;
		$('#pp_rna_evidence_table').dataTable(options);
	} 
	
	function set_up_single_table(data) {
		//Insert a 'details' column to the table  		
  		add_detail_column('evidence_table');
	
		//Set up cellular evidence table
		var options = {};
		options["bPaginate"] = true;
        options["oTableTools"] = table_tool_option('${phenotype_evidence_filename}', true, 3, 2);
		options["sDom"] = '<"clear">lfrtipT';
		options["aaSorting"] = [[0, "asc"]];
        options["fnInitComplete"] = set_up_message_and_count("evidence_header", "evidence_message", "evidence_wrapper");
		options["fnDrawCallback"] = set_up_details(get_pp_details);
		options["aoColumns"] = [{"bSortable":false}, null, {"bSearchable":false, "bVisible":false}, null, null, null, null, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, {"bSearchable":false, "bVisible":false}, null, {"bSearchable":false, "bVisible":false}, null]
		options["aaData"] = data;
		$('#evidence_table').dataTable(options);
	} 
	
	$(document).ready(function() {
	
		//Set up evidence table and reference list
  		$.getJSON('${phenotype_evidence_table_link}', function(data) {
			if('cellular_pheno' in data) {
				set_up_cell_table(data['cellular_pheno']);
				set_up_chem_table(data['chemical_pheno']);
				set_up_pp_table(data['pp_rna_pheno']);
  				
  				//Set total evidence count
				set_up_count(data['cellular_pheno'].length + data['chemical_pheno'].length + data['pp_rna_pheno'].length, "evidence_header");
			}
			else {
				set_up_single_table(data['aaData']);
				
				//Set total evidence count
				set_up_count(data['aaData'].length, "evidence_header");	
			}
			
			//Set up references
			set_up_references(data['reference'], 'reference_list');
  		});
	});
</script>

<script type="text/javascript">
	var vis = null;
window.onload=function() {
	vis = setup_go_cytoscape_vis("${phenotype_graph_link}");  
};           

</script>
        
<style>
	/* The Cytoscape Web container must have its dimensions set. */
	html, body { height: 80%; width: 100%; padding: 0; margin: 0; }
	#cytoscapeweb { width: 100%; height: 100%; }
</style>


</div>

</body>
</div>
</div>

