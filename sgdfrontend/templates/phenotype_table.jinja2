<!-- Phenotype
================================================== -->
<section id="phenotype" data-magellan-destination="phenotype">
	<div class="page-header">
		<h1>Phenotype Annotations
            <span class="round secondary label">
                <span id="phenotype_header"></span>
                <small id="phenotype_header_type"></small>
                <span id="phenotype_subheader"></span>
                <small id="phenotype_subheader_type"></small>
            </span>
		</h1>

		<div class="button-bar">
		    <ul class="button-group radius">
				<li><a disabled="true" id="phenotype_table_show_children" class="small button secondary" style="display:none"> Add Annotations to Child Terms</a></li>
			</ul>

			<ul class="button-group radius">
				<li><a disabled="true" id="phenotype_table_download" class="small button secondary"><i class="icon-download-alt"></i> Download</a></li>
			</ul>

			<ul class="button-group radius">
				<li><a disabled="true" id="phenotype_table_analyze" class="small button secondary"><i class="icon-briefcase"></i> Analyze</a></li>
			</ul>
		</div>

		<table class="table table-striped table-bordered table-condensed" id="phenotype_table" width="100%">
			<thead>
                <tr>
                    <th>Evidence ID</th>
                    <th>Analyze ID</th>
                    <th>Gene</th>
                    <th>Gene Format Name</th>
                    <th>Phenotype</th>
                    <th>Experiment Type</th>
                    <th>Mutant Information</th>
                    <th>Strain Background</th>
                    <th>Chemical</th>
                    <th>Details</th>
                    <th>Reference</th>
                </tr>
  			</thead>
		</table>
    </div>
</section>