<div metal:use-macro="layout">
  <div metal:fill-slot="content">


<body>

<!-- Subhead
================================================== -->
<header class="jumbotron subhead" id="overview">
	<div class="container">
		<h2><span tal:content="ref['citation']"></h2>
	</div>
</header>


<div class="container-fluid">

<!-- Docs nav
================================================== -->
<div class="row-fluid">
	<div class="span3 bs-docs-sidebar">
		<ul class="nav nav-list bs-docs-sidenav affix-top" id='navbar' name='navbar'>
			<li><h3><span tal:content="ref.display_name"></h3></li>
			<li><a href="#abstract"><i class="icon-chevron-right"></i> Abstract</a></li>
			<li><a href="#cited_for_annotations"><i class="icon-chevron-right"></i> Cited For Annotations</a></li>
		</ul>
	</div>
	
	<div class="span9">  

<!-- Abstract
================================================== -->
<section id="abstract">
	<div class="page-header">
		<h1>Abstract</h1>
		<div>
			<p tal:condition="python: ref['abstract']" tal:content="structure ref['abstract']"></p>
			<p tal:condition="python: ref['reftypes'] != ''"><strong>Reference Type: </strong><span tal:content="ref['reftypes']"></span></p>
			<p tal:condition="python: ref['authors'] != ''"><strong>Authors: </strong><span tal:content="structure ref['authors']"></span></p>
			<p tal:condition="python: ref['pubmed_id'] != ''"><strong>Pubmed ID: </strong><span tal:content="structure ref['pubmed_id']"></span></p>
			<p tal:condition="python: ref['related_references'] != ''"><strong>Related References: </strong><span tal:content="structure ref['related_references']"></span></p>
			<p id='primary_wrapper'><strong>Primary Literature For: </strong><span id="primary"></span></p>
			<p id='additional_wrapper'><strong>Additional Literature For: </strong><span id="additional"></span></p>
			<p id='review_wrapper'><strong>Review For: </strong><span id="review"></span></p>

			<p tal:condition="python: ref['urls'] != ''"><strong>External Links: </strong><span tal:content="structure ref['urls']"></span></p>
		</div>
	</div>
</section>


<section id="cited_for_annotations">
	<div class="page-header">
		<h1>Cited for Annotations</h1>
		<!-- GO
================================================== -->
		<div id='go_wrapper'>
		<h3>Gene Ontology <span id='go_header'></h3>
		<p id='go_message' style="display:none;">No gene ontology information curated from this reference.</p>		
			<table class='table table-striped table-bordered table-condensed tablesorter' id='go_table' width="100%">
			<thead>
  			<tr>
    			<th>GO Term</th>
    			<th>Gene</th>
    			<th>Gene Descripion</th>
    			<th>Evidence Codes</th>
  			</tr>
  			</thead>
			</table>
		</div>
		<br></br>

<!-- Phenotypes
================================================== -->
		<div id='all_phenotype_wrapper'>
		<h3>Phenotype <span id='phenotype_header'></h3>
		
		<!-- General Phenotypes
		========================================== -->
		<div id='phenotype_wrapper'>
		<h4>Cellular Phenotype <span id='cellular_phenotype_header'></h4>
		<p id='phenotype_message' style="display:none;">No general phenotype information curated from this reference.</p>
			<table class='table table-striped table-bordered table-condensed tablesorter' id='phenotype_table' width="100%">
			<thead>
  			<tr>
    			<th>Phenotype</th>
    			<th>Gene</th>
    			<th>Qualifier</th>
    			<th>Mutant Type</th>
  			</tr>
  			</thead>
			</table>
			<br></br>
		</div>
				

		<!-- Chemical Phenotypes
		========================================== -->
		<div id='chemical_phenotype_wrapper'>
		<h4>Chemical Phenotype <span id='chemical_phenotype_header'></h4>
		<p id='chemical_phenotype_message' style="display:none;">No chemical phenotype information curated from this reference.</p>
			<table class='table table-striped table-bordered table-condensed tablesorter' id='chemical_phenotype_table' width="100%">
			<thead>
  			<tr>
    			<th>Phenotype</th>
    			<th>Gene</th>
  				<th>Chemical</th>
    			<th>Qualifier</th>
    			<th>Mutant Type</th>
   			</tr>
  			</thead>
			</table>
			<br></br>
		</div>
				

		<!-- Protein Peptide and RNA Phenotypes
		========================================== -->
		<div id='pp_rna_phenotype_wrapper'>
		<h4>Protein/Peptide and RNA Phenotype <span id='pp_rna_phenotype_header'></h4>
		<p id='pp_rna_phenotype_message' style="display:none;">No protein/peptide and RNA phenotype information curated from this reference.</p>
			<table class='table table-striped table-bordered table-condensed tablesorter' id='pp_rna_phenotype_table' width="100%">
			<thead>
  			<tr>
    			<th>Phenotype</th>
    			<th>Gene</th>
    			<th>Reporter</th>
    			<th>Qualifier</th>
    			<th>Mutant Type</th>
  			</tr>
  			</thead>
			</table>
			<br></br>
		</div>
		</div>

<!-- Interactions
================================================== -->
		<div id='interaction_wrapper'>
		<h3>Interaction <span id='interaction_header'></h3>
		<p id='interaction_message' style="display:none;">No interaction information curated from this reference.</p>
		
		<!-- Brief
		========================================== -->
			<table class='table table-striped table-bordered table-condensed tablesorter' id='interaction_table' width="100%">
			<thead>
  			<tr>
    			<th>Interactor</th>
    			<th>Interactor</th>
    			<th>Genetic Evidence from this Reference</th>
    			<th>Physical Evidence from this Reference</th>
    			<th>Total Evidence from this Reference</th>
  			</tr>
  			</thead>
			</table>
		</div>
</section>


<script>
	
	function set_up_go_table() {
		var options = {};
		options["bPaginate"] = true;
        options["oTableTools"] = table_tool_option('${go_filename}', true, 0, 1);
		options["sDom"] = '<"clear">lfrtipT';
		options["aaSorting"] = [[0, "asc"]];
        options["fnInitComplete"] = set_up_message_and_count("go_header", "go_message", "go_wrapper");
		options["aoColumns"] = [null, null, {"bSearchable":false, "bVisible":false}, null]
		options["bProcessing"] = true
		options["sAjaxSource"] = '${go_overview_table_link}';
		
  		$('#go_table').dataTable(options);
	}
	
	function set_up_interaction_table() {
		var options = {};
		options["bPaginate"] = true;
        options["oTableTools"] = table_tool_option('${interaction_filename}', true, 0, 1);
		options["sDom"] = '<"clear">lfrtipT';
		options["aaSorting"] = [[0, "asc"]];
        options["fnInitComplete"] = set_up_message_and_count("interaction_header", "interaction_message", "interaction_wrapper");
		options["aoColumns"] = [null, null, {"bSearchable":false, "bVisible":false}, null]
		options["bProcessing"] = true
		options["sAjaxSource"] = '${interaction_overview_table_link}';
		
  		$('#interaction_table').dataTable(options);
	}
	
	function set_up_pheno_cell_table(data) {
		var options = {};
		options["bPaginate"] = true;
        options["oTableTools"] = table_tool_option('${cellular_phenotype_filename}', true, 0, 1);
		options["sDom"] = '<"clear">lfrtipT';
		options["aaSorting"] = [[0, "asc"]];
        options["fnInitComplete"] = set_up_message_and_count("cellular_phenotype_header", "phenotype_message", "phenotype_wrapper");
		options["aaData"] = data;
		
  		$('#phenotype_table').dataTable(options);
	}
	
	function set_up_pheno_chem_table(data) {
		var options = {};
		options["bPaginate"] = true;
        options["oTableTools"] = table_tool_option('${chemical_phenotype_filename}', true, 0, 1);
		options["sDom"] = '<"clear">lfrtipT';
		options["aaSorting"] = [[1, "asc"]];
        options["fnInitComplete"] = set_up_message_and_count("chemical_phenotype_header", "chemical_phenotype_message", "chemical_phenotype_wrapper");
		options["aaData"] = data;
		
  		$('#chemical_phenotype_table').dataTable(options);
	}
	
	function set_up_pheno_pp_table(data) {
		var options = {};
		options["bPaginate"] = true;
        options["oTableTools"] = table_tool_option('${pp_rna_phenotype_filename}', true, 0, 1);
		options["sDom"] = '<"clear">lfrtipT';
		options["aaSorting"] = [[1, "asc"]];
        options["fnInitComplete"] = set_up_message_and_count("pp_rna_phenotype_header", "pp_rna_phenotype_message", "pp_rna_phenotype_wrapper");
		options["aaData"] = data;
		
  		$('#pp_rna_phenotype_table').dataTable(options);
	}
	
	$(document).ready(function() {

		set_up_go_table();
  		set_up_interaction_table();
			
  		//Get phenoevidence data
  		$.getJSON('${phenotype_overview_table_link}', function(data) {
  			set_up_pheno_cell_table(data['cellular_pheno']);
  			set_up_pheno_chem_table(data['chemical_pheno']);
  			set_up_pheno_pp_table(data['pp_rna_pheno']);
			
			//Set total phenotype count
			var num_rows = data['cellular_pheno'].length + data['chemical_pheno'].length + data['pp_rna_pheno'].length;
			set_up_count(num_rows, "phenotype_header");
			
			if(num_rows == 0) {
				document.getElementById('all_phenotype_wrapper').style.display = 'none';
			}
  		});
  		
  		//Set up litguide data
  		$.getJSON('${bioent_overview_table_link}', function(data) {
  			var primary = data['primary'];
  			var additional = data['additional'];
  			var review = data['review'];
  			  			
  			if(primary != '') {
  				document.getElementById("primary").innerHTML = primary;
  			}
  			else {
  				document.getElementById("primary_wrapper").style.display = 'none';
  			}
  			
  			if(additional != '') {
  				document.getElementById("additional").innerHTML = additional;
  			}
  			else {
  				document.getElementById("additional_wrapper").style.display = 'none';
  			}
  			
  			if(review != '') {
  				document.getElementById("review").innerHTML = review;
  			}
  			else {
  				document.getElementById("review_wrapper").style.display = 'none';
  			}

  		});
	} );
</script>


</div>

</body>
</div>
</div>

