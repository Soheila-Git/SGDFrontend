<!-- Regulation
================================================== -->
<section id="{{table_type}}_regulation" data-magellan-destination="{{table_type}}_regulation">
	<div class="page-header">
		<h1>{{table_name}}
            <span class="round secondary label">
                <span id="{{table_type}}_regulation_header"></span>
                <small id="{{table_type}}_regulation_header_type"></small>
                <span id="{{table_type}}_regulation_subheader"></span>
                <small id="{{table_type}}_regulation_subheader_type"></small>
            </span>
		</h1>

		<div class="button-bar">
			<ul class="button-group radius">
				<li><a id="{{table_type}}_regulation_table_download" class="small button secondary"><i class="icon-download-alt"></i> Download</a></li>
			</ul>

			<ul class="button-group radius">
				<li><a id="{{table_type}}_regulation_table_analyze" class="small button secondary"><i class="icon-briefcase"></i> Analyze</a></li>
			</ul>
		</div>

		<table class="table table-striped table-bordered table-condensed" id="{{table_type}}_regulation_table" width="100%">
			<thead>
                <tr>
                    <th hidden>Evidence ID</th>
                    <th hidden>Analyze ID</th>
                    <th>Regulator</th>
                    <th hidden>Regulator Systematic Name</th>
                    <th>Target</th>
                    <th hidden>Target Systematic Name</th>
                    <th>Experiment</th>
                    <th>Conditions</th>
                    <th>Strain</th>
                    <th>Source</th>
                    <th>Reference</th>
                </tr>
  			</thead>
		</table>
    </div>
</section>